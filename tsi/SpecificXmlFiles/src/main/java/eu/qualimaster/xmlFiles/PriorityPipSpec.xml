<pipeline>
	<name>PriorityPip</name>
		<command>
			<name>resultsSubscribe</name>
			<request>
				<field sequence="0">resultsSubscribe</field>
				<field sequence="1">%listOfPairs%</field>
			</request>
			<response>
				<field sequence="0">resultsSubscribe_response</field>
				<field sequence="1">%status%</field>
				<field sequence="2">%result%</field>
			</response>
		</command>
		
		<command>
			<name>resultsUnsubscribe</name>
			<request>
				<field sequence="0">resultsUnsubscribe</field>
				<field sequence="1">%listOfPairs%</field>
			</request>
			<response>
				<field sequence="0">resultsUnsubscribe_response</field>
				<field sequence="1">%status%</field>
			</response>
		</command>
</pipeline>
